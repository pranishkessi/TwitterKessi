<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.si.twitterkessi">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.si.twitterkessi.ActivityInterest_List"/>
        <activity android:name="com.si.twitterkessi.ActivitySelf" />
        <activity
            android:name="com.si.twitterkessi.ActivityDashBoard"
            android:label="title_activity_dash_board"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.si.twitterkessi.ActivityCustom" />
        <activity android:name="com.si.twitterkessi.ActivityCamera" />
        <activity android:name="com.si.twitterkessi.ActivitySignUP" />
        <activity android:name="com.si.twitterkessi.MainActivity" />
        <activity android:name="com.si.twitterkessi.ActivityLogin" />
        <activity android:name="com.si.twitterkessi.ActivityPassword" />
        <activity android:name="com.si.twitterkessi.ActivityVerify" />
        <activity android:name="com.si.twitterkessi.ActivityLastSignup" />
        <activity android:name="com.si.twitterkessi.ActivitySplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>